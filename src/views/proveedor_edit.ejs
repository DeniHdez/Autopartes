<%- include ('partials/_header') %>
    <div class="container mt-5">

        <div class="row">
            <div class="col-md-5">
                <h3>Editar datos</h3>
                <div class="card">
                    <div class="card-body">
                        <form action="updateE/<%=data.id%>" method="post">
                            <div class="form-group">
                                <input type="text" name="nomEmpresa" value="<%=data.nomEmpresa%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="direccion" value="<%=data.direccion%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="ciudad" value="<%=data.ciudad%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="estado" value="<%=data.estado%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="codigoP" value="<%=data.codigoP%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="correoElec" value="<%=data.correoElec%>" class="form-control">
                            </div>
                            <div class="form-group">
                                <input type="text" name="telefono" value="<%=data.telefono%>" class="form-control">
                            </div>
                            <button type="submit" class="btn btn-secondary">Actualizar</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- include ('partials/_footer') %>